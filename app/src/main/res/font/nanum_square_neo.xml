<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <!--light-->
    <font
        android:font="@font/nanum_square_neo_light"
        android:fontStyle="normal"
        android:fontWeight="300"
        app:font="@font/nanum_square_neo_light"
        app:fontStyle="normal"
        app:fontWeight="300" />

    <!--regular-->
    <font
        android:font="@font/nanum_square_neo_regular"
        android:fontStyle="normal"
        android:fontWeight="400"
        app:font="@font/nanum_square_neo_regular"
        app:fontStyle="normal"
        app:fontWeight="400" />

    <!--bold-->
    <font
        android:font="@font/nanum_square_neo_bold"
        android:fontStyle="normal"
        android:fontWeight="500"
        app:font="@font/nanum_square_neo_bold"
        app:fontStyle="normal"
        app:fontWeight="500" />

    <!--extrabold-->
    <font
        android:font="@font/nanum_square_neo_extrabold"
        android:fontStyle="normal"
        android:fontWeight="700"
        app:font="@font/nanum_square_neo_extrabold"
        app:fontStyle="normal"
        app:fontWeight="700" />

    <!--heavy-->
    <font
        android:font="@font/nanum_square_neo_heavy"
        android:fontStyle="normal"
        android:fontWeight="800"
        app:font="@font/nanum_square_neo_heavy"
        app:fontStyle="normal"
        app:fontWeight="800" />
</font-family>